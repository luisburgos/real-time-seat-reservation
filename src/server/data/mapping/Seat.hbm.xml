<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping>
 
  <class name="server.domain.Seat" table="asiento">
        <id name="eventId" column="seat_id">
            <generator class="identity"/>
        </id>

        <property name="seatNumber" type="java.lang.Integer" column="number"/>
        <property name="state" type="java.lang.String" column="estado"/>

    </class>
  
</hibernate-mapping>
